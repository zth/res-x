{
  "name": "rescript-htmx",
  "version": "0.0.0",
  "scripts": {
    "start": "NODE_ENV=production bun run src/Demo.js",
    "build": "NODE_ENV=production && bun run build:vite && bun run build:res",
    "build:vite": "vite build",
    "build:res": "rescript",
    "clean:res": "rescript clean",
    "dev:res": "rescript build -w",
    "dev:server": "bun --watch run src/Demo.js",
    "dev:vite": "vite",
    "dev": "concurrently 'bun:dev:*'"
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@rescript/core": "^0.7.0",
    "concurrently": "^8.2.1",
    "fast-glob": "^3.3.1",
    "rescript-x": "../",
    "rescript": "11.1.0-rc.2",
    "vite": "^4.4.11",
    "rescript-bun": "0.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3"
  }
}
