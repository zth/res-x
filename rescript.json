{
  "jsx": {
    "mode": "automatic",
    "module": "react",
    "version": 4
  },
  "namespace": "res-x",
  "uncurried": true,
  "name": "rescript-x",
  "sources": {
    "dir": "src"
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".js",
  "bs-dependencies": ["@rescript/core", "rescript-bun"],
  "bsc-flags": [
    "-open RescriptCore",
    "-open RescriptBun",
    "-open RescriptBun.Globals"
  ]
}
