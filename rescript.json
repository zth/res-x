{
  "jsx": {
    "mode": "automatic",
    "module": "react",
    "version": 4
  },
  "uncurried": true,
  "name": "rescript-x",
  "sources": {
    "dir": "src"
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".js",
  "bs-dependencies": ["@rescript/core"],
  "bsc-flags": ["-open RescriptCore"]
}
